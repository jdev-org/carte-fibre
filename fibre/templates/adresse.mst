    <style>
        #item_{{statut_rg}}_{{feature_ol_uid}} > :not(#card_{{statut_rg}}_{{feature_ol_uid}}){
            display:none;
        }

        .xs #item_{{statut_rg}}_{{feature_ol_uid}} > #messageTplFibre_{{feature_ol_uid}}{
            display:block!important;
        }

        #right-panel .active {
            padding-bottom: 1em!important;
        }

        #right-panel .list-group-item > li {
            padding-bottom: 1em!important;
            list-style:none;
        }
        #right-panel .panel-heading {
            display: none;
        }

        #right-panel .panel, #right-panel .list-group-item, #right-panel .panel-body {
            border: none;
        }
        
        #right-panel .panel {
            box-shadow: none;
            margin-bottom: 8em
        }

        #modal-panel #dataToggleDiv {
            display: none;
            border:none
        }

        #modal-panel .panel {box-shadow: none;}

        #modal-panel .panel-body {
            border: none!important;
        }

        .item__card {
            border-radius: .4em;
        }

        .itemCard__header {      
            color:white;
            border-top-left-radius:.4em;
            border-top-right-radius:.4em;
            padding: .7em;
            font-size: 16px;
            font-weight: 600;
            display: flex;
            flex-direction: row;
            align-items: center;
            justify-content: space-between;
        }

        .itemCard__header img {
            margin-left: 1em;
            pointer-events: none;
        }

        .itemCard__body {
            padding: 1em;
            color:var(--mycolor);
        }

        .itemCard__title {
            font-size: 1.4em;
            font-weight: 600;
            line-height: 1;
            margin: 10px 0;
        }

        .itemCard__btn {
            display: flex;
            justify-content: center;
            border: 1px solid var(--mycolor);
            border-radius: 4px;
            color: var(--mycolor);
            padding: 4px;
            margin-bottom: 1em;
            transition: all .3s ease-out;
        }

        .itemCard__btn:hover {
            color: white;
            background-color: var(--mycolor);
        }

        .infoTech__btn {
            display: flex;
            align-items: center;
            flex-direction: row;
            flex-wrap: nowrap;
            justify-content: flex-end;
            color: var(--mycolor);
        }

        .infoTech__btn:hover {
            cursor:pointer;
            text-decoration:underline;
        }

        .infoTech__block {
            border-top: 1px solid #00378524;
            margin-top: 1em;
            padding: .5em 0 0 0;
        }

        .infoTech__btn svg {
            transition: .3s
        }

        .infoTech__btn.activeInfo svg {
            transform: rotate(90deg);
        }
        .messageTplFibre__xs{
            margin: 5em 0 1em;
            text-align: center;
        }
            
    </style>   
    <script>
        function displayInfoTech(btn,id) {
            let y = document.getElementById(btn);
            let x = document.getElementById(id);
            if (x.style.display === "none") {
                x.style.display = "block";
                y.classList.add("activeInfo");
            } else {
                x.style.display = "none";
                y.classList.remove("activeInfo");
            }
        }
    </script>
{{/features}}